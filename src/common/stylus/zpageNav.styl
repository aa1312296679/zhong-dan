.zpagenav {
  text-align: center;
  -webkit-user-select: none;
}

.zpagenav {
  font-family: arial;
  color: #48576a;
  overflow hidden
}

.page_total .total
  margin-right 10px

.zpagenav .page_total
  float left
  color #fff

.number_highlight
  color #00b2ff

.zpagenav .page_total,
.zpagenav ul
  margin: 20px 20px;
  padding: 0;
  display: block;

.zpagenav ul {
  float right;
}

.zpagenav ul li {
  display: block;
  float left;
  padding: 0 4px;
  min-width 18px;
  color #fff
  font-weight bold
  border: 1px solid #E2E2E2;
  font-size: 13px;
  height: 28px;
  line-height: 28px;
  cursor: pointer;
  box-sizing: border-box;
  text-align: center;
  margin 0 2px
}

.zpagenav ul li:last-child {
  border-right: 1px solid #d1dbe5;
}

.zpagenav ul li:hover {
  color: #20a0ff;
}

.zpagenav ul li.active {
  border-color: #20a0ff;
  background-color: #20a0ff;
  color: #fff;
  cursor: default;
}

.zpagenav ul li.active:hover {
  color: #fff;
}

.zpagenav ul li.disabled {
  cursor: not-allowed;
  color: #e4e4e4;
}

